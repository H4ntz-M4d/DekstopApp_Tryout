<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="485.0" prefWidth="746.0"
      spacing="15.0" styleClass="loginFrame" stylesheets="@../../../Style/styles.css" xmlns="http://javafx.com/javafx/17.0.2-ea"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.projectapk.Controller.LoginController">
   <children>
      <Label text="Sign In" textFill="WHITE">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
      </Label>
      <Label text="Username" textFill="WHITE">
         <font>
            <Font size="15.0" />
         </font>
         <VBox.margin>
            <Insets right="265.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="emailTextField1" maxWidth="-Infinity" onKeyReleased="#handleKeyRelease1" prefHeight="25.0" prefWidth="337.0">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <Label layoutX="217.0" layoutY="245.0" text="Password" textFill="WHITE">
         <font>
            <Font size="15.0" />
         </font>
         <VBox.margin>
            <Insets right="267.0" />
         </VBox.margin>
      </Label>
      <PasswordField fx:id="passwordTextField1" layoutX="215.0" layoutY="263.0" maxWidth="-Infinity" onKeyReleased="#handleKeyRelease1" prefHeight="25.0" prefWidth="337.0">
         <font>
            <Font size="15.0" />
         </font>
      </PasswordField>
      <Button fx:id="loginButton2" maxWidth="-Infinity" mnemonicParsing="false" prefHeight="40.0" prefWidth="121.0" styleClass="HomeButton" text="Login" textFill="WHITE">
         <VBox.margin>
            <Insets top="30.0" />
         </VBox.margin>
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Button>
      <Button fx:id="btnCancel" layoutX="322.0" layoutY="355.0" maxWidth="-Infinity" mnemonicParsing="false" prefHeight="40.0"
              prefWidth="121.0" styleClass="HomeButton" text="Cancel" textFill="WHITE" onAction="#cancel">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Button>
   </children>
</VBox>
